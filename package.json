{
    "name": "PathCopyTree",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "serve": "vite preview",
        "prebuild": "rm -rf dist/",
        "build": "vite build",
        "postbuild": "zip -r dist.zip dist/ && open .",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "build-only": "vite build",
        "dev": "vite --port 3000",
        "majorBuild": "yarn major & yarn build",
        "minorBuild": "yarn minor & yarn build",
        "patchBuild": "yarn patch & yarn build",
        "major": "npm version major & node sh/version_up_major.js & git add . & git commit -m 'major update'",
        "minor": "npm version minor & node sh/version_up_minor.js  & git add . & git commit -m 'major update' ",
        "patch": "npm version patch & node sh/version_up_patch.js & git add . & git commit -m 'major update' ",
        "preview": "vite preview --port 4173",
        "test": "vitest",
        "cytest": "yarn cypress open ",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@mdi/font": "5.9.55",
        "@quasar/cli": "^2.0.0",
        "@quasar/extras": "^1.15.11",
        "quasar": "^2.11.6",
        "roboto-fontface": "*",
        "ts-tree-structure": "^1.0.2",
        "vue": "^3.2.37",
        "vue-router": "^4.0.16",
        "vue3-tree": "^0.11.5",
        "vuetify": "^3.1.4",
        "webfontloader": "^1.0.0"
    },
    "devDependencies": {
        "@mdi/js": "^7.1.96",
        "@quasar/vite-plugin": "^1.3.0",
        "@rushstack/eslint-patch": "^1.1.0",
        "@types/chrome": "^0.0.190",
        "@types/jest": "^29.5.0",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.18",
        "@types/papaparse": "^5.3.7",
        "@types/webfontloader": "^1.0.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "babel-eslint": "^10.1.0",
        "cypress": "^12.8.1",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.3.0",
        "happy-dom": "^8.1.5",
        "husky": "^8.0.2",
        "jsdom": "^20.0.3",
        "lint-staged": "^13.1.0",
        "npm-run-all": "^4.1.5",
        "papaparse": "^5.3.2",
        "prettier": "^2.8.1",
        "sass": "1.32.12",
        "sass-loader": "^13.2.0",
        "start-server-and-test": "^1.15.2",
        "typescript": "~4.7.4",
        "vite": "^4.0.0",
        "vite-plugin-vuetify": "^1.0.0-alpha.12",
        "vitest": "^0.28.2",
        "vue-cli-plugin-vuetify": "~2.5.8",
        "vue-tsc": "^1.0.12"
    }
}
